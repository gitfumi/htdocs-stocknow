	<mt:SetVarBlock name="shopAddress4"><mt:CategoryShopAddress4 encode_html="1"></mt:SetVarBlock>
	<mt:SetVarBlock name="shopOverview4"><mt:CategoryShopOverview4 encode_html="1" remove_html="1" convert_breaks="0" nl2br="xhtml"></mt:SetVarBlock>
	<mt:CategoryOnlinePay setvar="onlinePay">
	<mt:var name="onlinePay" split=",","onlinePayArray">
	<mt:SetVarBlock name="onlinePay"><mt:CategoryOnlinePay encode_html="1"></mt:SetVarBlock>
	<mt:Var name="onlinePayArray">
</mt:if>
<mt:if name="parentTopCategory" eq="station">
	<mt:SetVarBlock name="station"><mt:CategoryLabel encode_html="1"></mt:SetVarBlock>
</mt:if>
</mt:entrycategories>

	<!-- .mainItem -->
	<div class="mainItem">
		<div class="itemInner">
			<div class="itemAlbum is-new">
				<div id="js-slickMain" class="albumMain">
					<mt:If tag="EntryImage"><mt:EntryImageAsset>
					<div class="mainItem"><img src="<mt:AssetURL>" alt=""></div></mt:EntryImageAsset></mt:If>
					<mt:If tag="EntryImageSub1"><mt:EntryImageSub1Asset>
					<div class="mainItem"><img src="<mt:AssetURL>" alt=""></div></mt:EntryImageSub1Asset></mt:If>
					<mt:If tag="EntryImageSub2"><mt:EntryImageSub2Asset>
					<div class="mainItem"><img src="<mt:AssetURL>" alt=""></div></mt:EntryImagesub2Asset></mt:If>
				</div>
				<div id="js-slickThum" class="albumList">
					<mt:If tag="EntryImage"><mt:EntryImageAsset>
					<div class="listItem"><img src="<mt:AssetURL>" alt=""></div></mt:EntryImageAsset></mt:If>
					<mt:If tag="EntryImageSub1"><mt:EntryImageSub1Asset>
					<div class="listItem"><img src="<mt:AssetURL>" alt=""></div></mt:EntryImageSub1Asset></mt:If>
					<mt:If tag="EntryImageSub2"><mt:EntryImageSub2Asset>
					<div class="listItem"><img src="<mt:AssetURL>" alt=""></div></mt:EntryImagesub2Asset></mt:If>
				</div>
			</div>

			<section class="itemHead">
				<mt:If name="brand"><p class="itemBrand"><mt:Var name="brand"></p></mt:If>
				<h1 class="itemName"><mt:EntryTitle encode_html="1"></h1>
				<mt:If tag="EntryPrice"><p class="itemPrice itemStatus is-frozen"><em><mt:EntryPrice sprintf="%d" numify="1"></em>円（税込）</p></mt:If>
				<div class="itemParam">
					<mt:If tag="EntryLimitDate"><p class="itemLimit"><span class="ttl">賞味期限</span><span class="txt"><mt:EntryLimitDate> days</span></p></mt:If>
					<mt:If name="station"><p class="itemStation"><span class="ttl">販売店</span><span class="txt"><mt:Var name="station">駅</span></p></mt:If>
				</div>
				<div class="itemCredit">
					<p class="creditTtl">オンラインショップの支払方法</p>
					<ul class="creditList">
						<li>銀行振込</li>
						<li>代金引換</li>
						<li>クレジットカード</li>
						<li>コンビニ決済</li>
					</ul>
				</div>
				<p class="itemBtn m-btn is-blank"><a href=""><span>オンラインショップへ</span></a></p>
			</section>
		</div>
	</div><!-- .mainItem -->

	<!-- .mainDetails -->
