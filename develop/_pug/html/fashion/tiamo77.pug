extends /_pug/_inc/_layout

//-
	---------------------------------------------------------
	ページ固有の変数を定義
	---------------------------------------------------------
append setVar
	- layout = 'entry'
	- pageUrl = '/fashion/tiamo77.html'
	- dirArray = createDirArray(pageUrl)
	- bodyAttributes.id = 'p-entry'
	- bodyAttributes.class = 'p-tiamo g-fashion'

//-
	---------------------------------------------------------
	ページ固有のCSSを読み込み
	---------------------------------------------------------
append css
	link(rel="stylesheet" href="/css/p_entry.css")

//-
	---------------------------------------------------------
	コンテンツ
	---------------------------------------------------------
block main

	article.main_inner

		header.entry_header
			div.mainvisual
				p.mainvisual_pic: img(src="/fashion/img/pic_tiamo77_01.jpg" alt="")
				p.mainvisual_date 2019.03.03
				p.mainvisual_episode 
				h1.mainvisual_ttl: span 《#77》2019 春夏スタート！<br>ティアモのゲキ推しアイテムプレビュー（後編）
			ul.c-tag_list
				li: a(href="/") #[span タグ1]
				li: a(href="/") #[span タグあああああ]
				li: a(href="/") #[span タグ3]
				li: a(href="/") #[span タグｆｆｆ]
				li: a(href="/") #[span タグ5]
				li: a(href="/") #[span タグ6]
				li: a(href="/") #[span タグ3]
				li: a(href="/") #[span タグｆｆｆ]
				li: a(href="/") #[span タグ5]
				li: a(href="/") #[span タグ6]

		div.c-advertise: a(href="/"): img(src="https://placehold.it/728x90" alt="")

		div.entry_content
			div.mokuji
				p.m-titleType03 目次
				ul.mokuji_list
					li: a(href="#anchor01") [動画] 2019SSスタート！ティアモのゲキ推しアイテムプレビュー（後編）
					li: a(href="#anchor02") 春セレクト！！ネクタイとネッカチーフ♪
					li: a(href="#anchor03") 今季のトレンドパンツ！！
					li: a(href="#anchor04") 幅広く使えるジャケット＆コート！！
					li: a(href="#anchor05") 嬉しい3ピースセットアップ！！
					li: a(href="#anchor06") その他・特典情報

			section.entry_section#anchor01
				h2.m-titleType02 [動画] 2019SSスタート！ティアモのゲキ推しアイテムプレビュー（後編）
				div.entry_youtube
					iframe(width="560" height="315" src="https://www.youtube.com/embed/cwE6QWbS-Rg" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen)
			
			section.entry_section#anchor02
				h2.m-titleType02 春セレクト！！ネクタイとネッカチーフ♪
				p.entry_txt ネッカチーフの注目は今年もキンロックの別注品！テーマはメキシコ！キンロック製品はシルクで巻きやすくて使いやすいのが特徴です。アルテアの新作も登場しています。<br>今年のネクタイは　主流の”ネイビー”　×　バブル時代の”イエロー”　掛け合わせがポイント。
				div.entry_saleitems
					section: a(target="_blank" href="https://store.nanouniverse.jp/jp/g/g6709120012_r1110_y140")
						h3.saleitems_name サークルストライプオープンカラーシャツ
						p.saleitems_maker Bagutta
						p.saleitems_pic: img(src="/fashion/img/pic_tiamo77_item01.jpg" alt="サークルストライプオープンカラーシャツ")
					section: a(target="_blank" href="https://store.nanouniverse.jp/jp/g/g6709120013_r1110")
