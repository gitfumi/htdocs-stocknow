extends /_pug/_inc/_layout

//-
	---------------------------------------------------------
	ページ固有の変数を定義
	---------------------------------------------------------
append setVar
	- layout = 'entry'
	- pageUrl = '/fashion/tiamo78.html'
	- dirArray = createDirArray(pageUrl)
	- bodyAttributes.id = 'p-entry'
	- bodyAttributes.class = 'p-tiamo g-fashion'

//-
	---------------------------------------------------------
	ページ固有のCSSを読み込み
	---------------------------------------------------------
append css
	link(rel="stylesheet" href="/css/p_entry.css")

//-
	---------------------------------------------------------
	コンテンツ
	---------------------------------------------------------
block main

	nav.m-breadcrumbs
		ol.m-breadcrumbs_list
			li: a(href="/") HOME
			li 《#78》バブるバブればバブる時！春のバブアーも別注で決めろ！

	article.main_inner

		header.entry_header
			div.mainvisual
				p.mainvisual_pic: span: img(src="/fashion/img/pic_tiamo78_01.jpg" alt="")
				p.mainvisual_date 2019.03.12
				p.mainvisual_episode 
				h1.mainvisual_ttl: span 《#78》バブるバブればバブる時！<br>春のバブアーも別注で決めろ！
			ul.c-tag_list
				li: a(href="/") #[span タグ1]
				li: a(href="/") #[span タグあああああ]
				li: a(href="/") #[span タグ3]
				li: a(href="/") #[span タグｆｆｆ]
				li: a(href="/") #[span タグ5]
				li: a(href="/") #[span タグ6]
				li: a(href="/") #[span タグ3]
				li: a(href="/") #[span タグｆｆｆ]
				li: a(href="/") #[span タグ5]
				li: a(href="/") #[span タグ6]

		//#include virtual="/_inc/advertise780.html"

		div.entry_content
			div.mokuji
				p.m-titleType03 目次
				ul.mokuji_list
					li: a(href="#anchor01") [動画] 《#78》バブるバブればバブる時！春のバブアーも別注で決めろ！
					li: a(href="#anchor02") 去年の秋冬でも登場♪コーディラナイロン生地の「ビデイル（BEDALE）」
					li: a(href="#anchor03") 新型のナイロンタイプのロング「ニューバーレイ」と「カモフラージュ柄」！！

			section.entry_section#anchor01
				h2.m-titleType02 [動画] 《#78》バブるバブればバブる時！春のバブアーも別注で決めろ！
				div.entry_youtube
					iframe(width="560" height="315" src="https://www.youtube.com/embed/_iib9ss3jGk" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen)

			section.entry_section#anchor02
				h2.m-titleType02 去年の秋冬でも登場♪コーディラナイロン生地の「ビデイル（BEDALE）」
				p.entry_txt 昨年同様コーデュラナイロンを使用しつつもボタンのカラーをマットブラック仕様に変更、インナーベストが脱着できるよう内側にファスナーを追加 することでちょっとした見た目の変化と汎用性を考慮した1着。カラーバイレーションはベーシック「オリーブ」と「ブラック」。
				div.entry_saleitems
					section: a(target="_blank" rel="nofollow" href="http://h.accesstrade.net/sp/cc?rk=0100l47v00j55a&url=https%3A%2F%2Fstore.nanouniverse.jp%2Fjp%2Fg%2Fg6709111010_r1310_y310")
						h3.saleitems_name 別注BEDALE　SL　コーデュラ オリーブ
						p.saleitems_maker Barbour
						p.saleitems_pic: img(src="/fashion/img/pic_tiamo78_item01.jpg" alt="別注BEDALE　SL　コーデュラ オリーブ")
					section: a(target="_blank" rel="nofollow" href="http://h.accesstrade.net/sp/cc?rk=0100l47v00j55a&url=https%3A%2F%2Fstore.nanouniverse.jp%2Fjp%2Fg%2Fg6709111010_r1310_y310")
						h3.saleitems_name 別注BEDALE　SL　コーデュラ ブラック
						p.saleitems_maker Barbour
						p.saleitems_pic: img(src="/fashion/img/pic_tiamo78_item02.jpg" alt="別注BEDALE　SL　コーデュラ ブラック")

			
			section.entry_section#anchor03
				h2.m-titleType02 新型のナイロンタイプのロング「ニューバーレイ」と「カモフラージュ柄」！！
				p.entry_txt ステンカラーコートタイプのバーレイは通常乗馬用に使われていたもの。今回のニューバーレイモデルは日常用にアレンジを加えたモデルになります。もう一つもナイロンタイプですが、オリジナルのカモフラージュ柄がポイント。
				div.entry_saleitems
					section: a(target="_blank" rel="nofollow" href="http://h.accesstrade.net/sp/cc?rk=0100l47v00j55a&url=https%3A%2F%2Fstore.nanouniverse.jp%2Fjp%2Fg%2Fg6709111011_r1310_y141")
						h3.saleitems_name 別注NEW BURGHLEY（ニューバーレイ）
						p.saleitems_maker Barbour
						p.saleitems_pic: img(src="/fashion/img/pic_tiamo78_item03.jpg" alt="別注NEW BURGHLEY（ニューバーレイ）")
					section: a(target="_blank" rel="nofollow" href="http://h.accesstrade.net/sp/cc?rk=0100l47v00j55a&url=https%3A%2F%2Fstore.nanouniverse.jp%2Fjp%2Fg%2Fg6709111012_r1316_y010")
						h3.saleitems_name 別注MODIFIED INTERNATIONAL SL（モディファイ　インターナショナル）
						p.saleitems_maker Barbour
						p.saleitems_pic: img(src="/fashion/img/pic_tiamo78_item04.jpg" alt="別注MODIFIED INTERNATIONAL SL（モディファイ　インターナショナル）")
				
						
