extends /_pug/_inc/_layout

//-
	---------------------------------------------------------
	ページ固有の変数を定義
	---------------------------------------------------------
append setVar
	- layout = 'entry'
	- pageUrl = '/fashion/tiamo79.html'
	- dirArray = createDirArray(pageUrl)
	- bodyAttributes.id = 'p-entry'
	- bodyAttributes.class = 'p-tiamo g-fashion'

//-
	---------------------------------------------------------
	ページ固有のCSSを読み込み
	---------------------------------------------------------
append css
	link(rel="stylesheet" href="/css/p_entry.css")

//-
	---------------------------------------------------------
	コンテンツ
	---------------------------------------------------------
block main

	article.main_inner

		header.entry_header
			div.mainvisual
				p.mainvisual_pic: img(src="/fashion/img/pic_tiamo79_01.jpg" alt="")
				p.mainvisual_date 2019.03.15
				p.mainvisual_episode 
				h1.mainvisual_ttl: span 《#79》今年のシャツも個性派揃い！<br>春の新作イッキ見せ！(前編)
			ul.c-tag_list
				li: a(href="/") #[span タグ1]
				li: a(href="/") #[span タグあああああ]
				li: a(href="/") #[span タグ3]
				li: a(href="/") #[span タグｆｆｆ]
				li: a(href="/") #[span タグ5]
				li: a(href="/") #[span タグ6]
				li: a(href="/") #[span タグ3]
				li: a(href="/") #[span タグｆｆｆ]
				li: a(href="/") #[span タグ5]
				li: a(href="/") #[span タグ6]

		//#include virtual="/_inc/advertise780.html"

		div.entry_content
			div.mokuji
				p.m-titleType03 目次
				ul.mokuji_list
					li: a(href="#anchor01") [動画] 《#79》今年のシャツも個性派揃い！春の新作イッキ見せ！(前編)
					li: a(href="#anchor02") バカンスを思わせるリラックス感が漂う「バカンツァ・シャツ」
					li: a(href="#anchor03") 春らしい、爽やかさのあるのストライプシャツ！！

			section.entry_section#anchor01
				h2.m-titleType02 [動画] 《#79》今年のシャツも個性派揃い！春の新作イッキ見せ！(前編)
				div.entry_youtube
					iframe(width="560" height="315" src="https://www.youtube.com/embed/6DdMX7JV6jY" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen)

			section.entry_section#anchor02
				h2.m-titleType02 バカンスを思わせるリラックス感が漂う「バカンツァ・シャツ」
				p.entry_txt リラックスな素材を使用したリゾート感漂うシャツ。シルエットも春らしいカラー（※シャツの襟）がないスタンドカラー（※バンドカラー）でバカンツァシャツだけでもサラッとおしゃれに仕上がります。
				div.entry_saleitems
					section: a(target="_blank" href="http://h.accesstrade.net/sp/cc?rk=0100l47v00j55a&url=https%3A%2F%2Fstore.nanouniverse.jp%2Fjp%2Fg%2Fg6709120034_r1110_y030")
						h3.saleitems_name バカンツァ・シャツ１１
						p.saleitems_maker DNL
						p.saleitems_pic: img(src="/fashion/img/pic_tiamo79_item01.jpg" alt="バカンツァ・シャツ１１")
					section: a(target="_blank" href="http://h.accesstrade.net/sp/cc?rk=0100l47v00j55a&url=https%3A%2F%2Fstore.nanouniverse.jp%2Fjp%2Fg%2Fg6709120024_r1110_y360")
						h3.saleitems_name バカンツァ・シャツ１
						p.saleitems_maker DNL
						p.saleitems_pic: img(src="/fashion/img/pic_tiamo79_item02.jpg" alt="バカンツァ・シャツ１")

			
			section.entry_section#anchor03
				h2.m-titleType02 春らしい、爽やかさのあるのストライプシャツ！！
				p.entry_txt ロンドンストライプ柄のカッタウェイシャツが登場。ウエストを絞っているので体にフィットしスタイリッシュに見えます。丈も短めなのでボタンも開けても◎。
				div.entry_saleitems
					section: a(target="_blank" href="http://h.accesstrade.net/sp/cc?rk=0100l47v00j55a&url=https%3A%2F%2Fstore.nanouniverse.jp%2Fjp%2Fg%2Fg6709120036%2F")
						h3.saleitems_name ロンドンストライプカッタウェイシャツ
						p.saleitems_maker giannetto
						p.saleitems_pic: img(src="/fashion/img/pic_tiamo79_item03.jpg" alt="ロンドンストライプカッタウェイシャツ")
					section: a(target="_blank" href="http://h.accesstrade.net/sp/cc?rk=0100l47v00j55a&url=https%3A%2F%2Fstore.nanouniverse.jp%2Fjp%2Fg%2Fg6709120012_r1110_y140")
						h3.saleitems_name サークルストライプオープンカラーシャツ
						p.saleitems_maker Bagutta
						p.saleitems_pic: img(src="/fashion/img/pic_tiamo79_item04.jpg" alt="サークルストライプオープンカラーシャツ")
				
						
