extends /_pug/_inc/_layout

//-
	---------------------------------------------------------
	ページ固有の変数を定義
	---------------------------------------------------------
append setVar
	- layout = 'entry'
	- pageUrl = '/fashion/tiamo82.html'
	- dirArray = createDirArray(pageUrl)
	- bodyAttributes.id = 'p-entry'
	- bodyAttributes.class = 'p-tiamo g-fashion'

//-
	---------------------------------------------------------
	ページ固有のCSSを読み込み
	---------------------------------------------------------
append css
	link(rel="stylesheet" href="/css/p_entry.css")

//-
	---------------------------------------------------------
	コンテンツ
	---------------------------------------------------------
block main

	article.main_inner

		header.entry_header
			div.mainvisual
				p.mainvisual_pic: img(src="/fashion/img/pic_tiamo82_01.jpg" alt="")
				p.mainvisual_date 2019.04.04
				p.mainvisual_episode 
				h1.mainvisual_ttl: span 《#82》ティアモVS広瀬 徹底討論！<br>この春、１番モテるジャケットはどれだ？
			ul.c-tag_list
				li: a(href="/") #[span タグ1]
				li: a(href="/") #[span タグあああああ]
				li: a(href="/") #[span タグ3]
				li: a(href="/") #[span タグｆｆｆ]
				li: a(href="/") #[span タグ5]
				li: a(href="/") #[span タグ6]
				li: a(href="/") #[span タグ3]
				li: a(href="/") #[span タグｆｆｆ]
				li: a(href="/") #[span タグ5]
				li: a(href="/") #[span タグ6]

		//#include virtual="/_inc/advertise780.html"

		div.entry_content
			div.mokuji
				p.m-titleType03 目次
				ul.mokuji_list
					li: a(href="#anchor01") [動画] 《#82》ティアモVS広瀬 徹底討論！この春、１番モテるジャケットはどれだ？
					li: a(href="#anchor02") 抑えておくべきセットアップのトレンド傾向
					li: a(href="#anchor03") 紺のブレザーのイメージが強い！アメトラのブルックスブラザーズ

			section.entry_section#anchor01
				h2.m-titleType02 [動画] 《#82》ティアモVS広瀬 徹底討論！この春、１番モテるジャケットはどれだ？
				div.entry_youtube
					iframe(width="560" height="315" src="https://www.youtube.com/embed/euGMU2wTWdc" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen)

			section.entry_section#anchor02
