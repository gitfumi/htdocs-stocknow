	article.main_inner

		header.entry_header
			div.mainvisual
				p.mainvisual_pic: img(src="/fashion/img/pic_tiamo86_01.jpg" alt="")
				p.mainvisual_date 2019.04.25
				p.mainvisual_episode 
				h1.mainvisual_ttl: span 《#86》ティアモチルドレンを探せ！（ラゾーナ川崎店編）
			ul.c-tag_list
				li: a(href="/") #[span タグ1]
